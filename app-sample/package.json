{
  "name": "@rchitect/app-sample",
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm rimraf dist && pnpm vite build --mode $NODE_ENV",
    "clean": "pnpm rimraf node_modules && pnpm rimraf dist && pnpm rimraf .turbo",
    "dev": "vite -m development",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "report": "pnpm cross-env REPORT=true npm run build",
    "server:br": "pnpx http-server dist --cors --brotli -c-1",
    "server:gzip": "pnpx http-server dist --cors --gzip -c-1",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@rchitect-app/account": "workspace:^",
    "@rchitect-app/component-driver-naive": "workspace:^",
    "@rchitect-app/infrastructure": "workspace:^",
    "@rchitect-design/constants": "workspace:^",
    "@rchitect-design/styles": "workspace:^",
    "@rchitect-rock/base-package": "workspace:^",
    "@rchitect-rock/components": "workspace:^",
    "@rchitect-rock/events": "workspace:^",
    "@rchitect-rock/hooks": "workspace:^",
    "@rchitect-rock/ioc": "workspace:^",
    "@rchitect-rock/layouts": "workspace:^",
    "@rchitect-rock/locale": "workspace:^",
    "@rchitect-rock/router": "workspace:^",
    "@rchitect-rock/settings": "workspace:^",
    "@rchitect-rock/state": "workspace:^",
    "axios": "^1.6.2",
    "dayjs": "^1.11.7",
    "pinia": "~2.1.7",
    "qs": "^6.11.2",
    "vue": "~3.3.4 || ~2.7.0",
    "vue-demi": "~0.14.6",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rchitect-cli/css-preprocess": "workspace:*",
    "@rchitect-cli/vite": "workspace:*",
    "@rchitect-design/types": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.7",
    "@types/nprogress": "^0.2.0",
    "@vue/compiler-sfc": "~3.2.0",
    "@vue/test-utils": "^2.2.9",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.34.3",
    "rollup": "^3.19.1",
    "typescript": "^5.3.3",
    "vite": "~4.5.0",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^1.0.24"
  }
}